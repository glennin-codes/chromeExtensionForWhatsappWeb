{
    "manifest_version": 2,
    "name": "WhatsApp Status Updater",
    "description": "A browser extension for updating your WhatsApp status",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["activeTab"],
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Update WhatsApp Status"
    }
  }
  